  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch no extras",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "env": {},
      "args": ["-config","./piclock.vscode.conf"],
      "buildFlags": "-tags=noaudio"
    },
    {
      "name": "Launch audio",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "env": {},
      "args": ["-config","./piclock.vscode.conf"],
      "buildFlags": ""
    },
    {
      "name": "Launch test",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceRoot}",
      "env": {},
      "buildFlags": "-tags=noaudio"
    },
    {
      "name": "Attach",
      "type": "go",
      "request": "attach",
      "remotePath": "${workspaceFolder}",
      "mode": "remote",
      "port": 2345,
      "host": "127.0.0.1",
      "apiVersion" : 1
    },
    {
      "name": "oauth",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "env": {},
      "args": ["-oauth", "-config","./piclock.vscode.conf"],
      "buildFlags": ""
    }
  ]
}